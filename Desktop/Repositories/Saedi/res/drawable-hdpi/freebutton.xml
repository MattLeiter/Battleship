<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#191970" />
      <stroke
          android:width="1dp"
          android:color="#FFA500" />
      <corners
          android:radius="2dp" />
      <padding
          android:left="1dp"
          android:top="1dp"
          android:right="1dp"
          android:bottom="1dp" />
    </shape>
  </item>
  <item>
    <shape>
      <solid android:color="#191970" />
      <stroke
          android:width="1dp"
          android:color="#1E90FF" />
      <corners
          android:radius="20dp" />
      <padding
          android:left="10dp"
          android:top="6dp"
          android:right="9dp"
          android:bottom="3dp" />
    </shape>
  </item>
</selector>